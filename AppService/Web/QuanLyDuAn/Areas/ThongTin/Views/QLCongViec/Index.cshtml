<div class="row col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12" id="div_container">
        <div id="container" class="elevation-2 col-lg-12 col-md-12 col-sm-12 col-xs-12"></div>
    </div>
    <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12" id="div_container_detail">
        <div id="container_step" class="elevation-2 col-lg-12 col-md-12 col-sm-12 col-xs-12"></div>
        <div id="container_attachment" class="elevation-2 col-lg-12 col-md-12 col-sm-12 col-xs-12"></div>
        <div id="container_schedule" class="elevation-2 col-lg-12 col-md-12 col-sm-12 col-xs-12"> </div>
        <div id="container_conversation" class="elevation-2 col-lg-12 col-md-12 col-sm-12 col-xs-12"> </div>

    </div>
</div>
<style>
    #container_step .dx-link-delete {
        margin-right: 15px !important;
    }

    #container_step {
        margin-bottom: 7px;
    }

    #container_attachment {
        margin-bottom: 7px;
    }

    #container_schedule {
        margin-bottom: 7px;
    }

    .dx-editor-container .dx-dropdowneditor-input-wrapper .dx-texteditor-input-container .dx-placeholder {
        color: white !important;
    }

    #container_conversation .dx-toolbar .dx-toolbar-items-container {
        height: 33px !important;
    }

    #container_schedule .dx-toolbar .dx-toolbar-items-container {
        height: 33px !important;
    }

    /*  .card-header {
        background-color: #ffd6ae !important;
    }*/
</style>

@section script{
    <script src="~/Scripts/page/thongtin.js"></script>
    <script>
        //------------------- Parameter ------------------------
        var NAMECOVNER = "Conversation";
        var NAMEPLANMASTER = "PlanMasterAccount";
        var NAMEPLANSCHEDULE = "PlanSchedule";
        var WIDTH_CONTAINER = $("#container").width();

        if (GROUPOWNERID !== 0 && GROUPOWNERID !== 7 && GROUPOWNERID !== 9) {
            GROUPOWNERID = 0;
            localStorage.setItem('groupOwnerIdCurrent', GROUPOWNERID);
        }
        //--------------------READ------------------------------
        const ACTION = "/PlanMaster/GetWithAccountProgress";
        const ACTION_DETAIL = "/PlanJob/GetWithAccount";
        const ACTION_SCHEDULE = "/PlanSchedule";
        const ACTION_ACCOUNTINFO = "/AccountInfo";
        const ACTION_GROUPS = "/Groups";
        const ACTION_PLAN_ACCOUNT = "/PlanAccount";
        const ACTION_FILEGET = "/FilesAttachment/getBy";
        const ACTION_CONVERSATION = "/Conversation";


        //--------------------CMD------------------------------
        const ACTION_CMD = "/PlanMaster";
        const ACTION_CMD_DETAIL = "/PlanJob";
        const ACTION_CMD_DETAILACC = "/PlanJobAccount";
        const ACTION_CMD_SCHEDULE = "/PlanSchedule";
        const ACTION_CMD_PLAN_ACCOUNT = "/PlanAccount";
        const ACTION_CMD_CONVERSATION = "/Conversation";



        //-------------------- CustomStore -------------------------
        let customStore = (gid) => {
            var fillterCustomStore = [["pmIsDelete", "<>", true], "and", ["PlanProjectId", "=", 3], "and", ["createBy", "=", UserCurrentInfo.accountId]];
            if (gid > 0) {
                fillterCustomStore.push("and", ["paPermistionId", "=", gid]);
            }
            return new DevExpress.data.DataSource({
                store: new DevExpress.data.CustomStore({
                    key: "paId",
                    load: (values) => ajax_read(NAMEPLANMASTER, values),
                    insert: (values) => ajax_insert(URL_API_PM_CMD + ACTION_CMD, values),
                    update: (key, values) => ajax_update(URL_API_PM_CMD + ACTION_CMD, key, values),
                    remove: (key) => ajax_delete(URL_API_PM_CMD + ACTION_CMD, key),
                }),
                filter: fillterCustomStore,
            })
        };
        let customStore_DETAIL = (id) => new DevExpress.data.DataSource({
            store: new DevExpress.data.CustomStore({
                key: "id",
                load: (values) => ajax_read(ACTION_CMD_DETAILACC, values),
                insert: (values) => ajax_insert(URL_API_PM_CMD + ACTION_CMD_DETAIL, values),
                update: (key, values) => ajax_update(URL_API_PM_CMD + ACTION_CMD_DETAIL, key, values),
                remove: (key) => ajax_delete(URL_API_PM_CMD + ACTION_CMD_DETAIL, key),
            }),
            filter: [["planMasterId", "=", id], "and", ["createby", "=", UserCurrentInfo.accountId]],
        });
        let customStore_PLANJOB_ACCOUNT = (id) => customStore_CMD_READ_FILTER_ID_KEYWORD(ACTION_CMD_PLAN_ACCOUNT, ACTION_PLAN_ACCOUNT, 'ownerById,' + id, 'PlanJob');
        let customStore_PLANMASTER_ACCOUNT = (id) => customStore_CMD_READ_FILTER_ID(ACTION_CMD_PLAN_ACCOUNT, ACTION_PLAN_ACCOUNT, "ownerById," + id + ";OwnerTable,'PlanMaster'");
        let customStore_Conver = (id) => new DevExpress.data.DataSource({
            store: new DevExpress.data.CustomStore({
                key: "id",
                load: (values) => ajax_read(NAMECOVNER, values),
                insert: (values) => ajax_insert(URL_API_PM_CMD + ACTION_CMD_CONVERSATION, values),
                update: (key, values) => ajax_update(URL_API_PM_CMD + ACTION_CMD_CONVERSATION, key, values),
                remove: (key) => ajax_delete(URL_API_PM_CMD + ACTION_CMD_CONVERSATION, key),
            }),
            filter: [["topicId", "=", id], "and", ["OwnerTable", "=", "PlanMaster"], "and", ["IsDelete", "=", 0]],
        });
        let customStore_Schelude = (id) => new DevExpress.data.CustomStore({
            key: "id",
            load: (values) => {
                values["filter"] = [["PlanMasterId", "=", id], "and", ["CreateBy", "=", UserCurrentInfo.accountId]];
                return ajax_read(NAMEPLANSCHEDULE, values);
            },
            insert: (values) => { console.log(values); ajax_insert(URL_API_PM_CMD + ACTION_SCHEDULE, values) },
            update: (key, values) => { console.log(values); ajax_update(URL_API_PM_CMD + ACTION_SCHEDULE, key, values); },
            //remove: (key) => ajax_delete(URL_API_PM_CMD + ACTION, key)
        });
        let customStore_AccountInfo = new DevExpress.data.CustomStore({
            key: "accountId",
            load: (values) => {
                var deferred = $.Deferred();
                $.ajax({
                    headers: header, dataType: "json",
                    url: URL_API_PM_READ + ACTION_ACCOUNTINFO,
                    success: function (data) {
                        var list = data.result.items;
                        deferred.resolve(list);
                    },
                    error: function (xhr, textStatus, errorThrown) {
                        console.log(xhr.responseJSON);
                        deferred.reject("Có lỗi xảy ra trong quá trình lấy danh sách 'Giai Đoạn'. Mở Console để xem chi tiết.");
                    },
                    timeout: 10000
                });
                return deferred.promise();
            },
        });
        let customStore_Groups = new DevExpress.data.CustomStore({
            key: "id",
            load: (values) => {
                var deferred = $.Deferred();
                $.ajax({
                    headers: header, dataType: "json",
                    url: URL_API_ACC_READ + ACTION_GROUPS,
                    success: function (data) {
                        var list = data.result.items.filter(x => x.isActive == true && x.isVisible == true);
                        deferred.resolve(list);
                    },
                    error: function (xhr, textStatus, errorThrown) {
                        console.log(xhr.responseJSON);
                        deferred.reject("Có lỗi xảy ra trong quá trình lấy danh sách 'Projects'. Mở Console để xem chi tiết.");
                    },
                    timeout: 10000
                });
                return deferred.promise();
            },
        });
        let customStore_File = (table, item) => new DevExpress.data.CustomStore({
            key: "id",
            load: (values) => {
                var deferred = $.Deferred();
                var params = {
                    'PageSize': isNullOrEmpty(values.take) ? values.take : 0,
                    'PageNumber': (isNullOrEmpty(values.take) && isNullOrEmpty(values.skip)) ? ((values.skip / values.take) + 1) : 0,
                    'FindParentId': table,
                    'FindId': item.id,
                }
                if (values.sort) {
                    params['SortCol'] = values.sort[0].selector;
                    params['SortADSC'] = values.sort[0].desc;
                }
                $.ajax({
                    headers: header, dataType: "json",
                    data: params,
                    url: URL_API_PM_READ + ACTION_FILEGET,
                    success: function (data) {
                        var list = data.result.items;
                        deferred.resolve(
                            list,
                            {
                                totalCount: list.length,
                            }
                        );
                    },
                    error: function (xhr, textStatus, errorThrown) {
                        console.log(xhr.responseJSON);
                        deferred.reject("Có lỗi xảy ra trong quá trình lấy danh sách 'Hạng mục'. Mở Console để xem chi tiết.");
                    },
                    timeout: 10000
                });
                return deferred.promise();
            },

        });

        //-------------------- Function ----------------------------
        $(function () {
            loadData(GROUPOWNERID);
        });

        //-------------------- Load --------------------------------
        let loadData = (id) => {
            $("#container").dxDataGrid({
                height: heightScreen,
                dataSource: customStore(id),
                repaintChangesOnly: true,
                remoteOperations: true,
                scrolling: { mode: "standard" },
                showBorders: false,
                showColumnHeaders: true,
                showColumnLines: false,
                hoverStateEnabled: true,
                showRowLines: true,
                columnAutoWidth: true,
                wordWrapEnabled: true,
                rowAlternationEnabled: true,
                filterRow: { visible: true },
                columns: [
                    {
                        dataField: "pmIsVisible",
                        caption: "Hoàn thành",
                        dataType: "boolean",
                        groupIndex: 0,
                        groupCellTemplate: (c, o) => {
                            o.value ? c.append($("<div>").addClass("text-success card-header-custom").append("Đã Hoàn Thành(" + o.summaryItems[0].value + ")")) : c.append($("<div>").addClass("text-danger card-header-custom").append("Chưa Hoàn Thành(" + o.summaryItems[0].value + ")"));
                        },
                        editorType: "dxCheckBox",
                        sortOrder: "desc"
                    },
                    {
                        dataField: "code",
                        width: "10%",
                        caption: "Mã",
                        dataType: "string",
                    },
                    {
                        dataField: "title",
                        width: "20%",
                        caption: "Tên kế hoạch CV",
                        dataType: "string",
                        validationRules: [{ type: "required" }],
                    },
                    {
                        dataField: "description",
                        width: "30%",
                        caption: "Mô tả CV",
                        dataType: "string",
                        //visible: false,
                    },
                    {
                        dataField: "importantLevel",
                        caption: "Độ quan trọng",
                        dataType: "number",
                        lookup: {
                            dataSource: important,
                            valueExpr: "ID",
                            displayExpr: "Name",
                        },
                        editorType: "dxSelectBox",
                        editorOptions: {
                            value: "importantLevel",
                            dataSource: important,
                            valueExpr: "ID", displayExpr: "Name",
                        },
                    },
                    //{
                    //    dataField: "priority",
                    //    caption: "Độ ưu tiên",
                    //    dataType: "number",
                    //},
                    {

                        dataField: "note",
                        label: { text: "Lưu ý" },
                        editorType: "dxTextArea",
                        visible: false,
                    },
                    {
                        dataField: "startDate",
                        label: { text: "Ngày bắt đầu" },
                        visible: false,
                        editorType: "dxDateBox",
                        editorOptions: {
                            pickerType: "rollers",
                            type: "datetime",
                        },
                    },
                    {

                        dataField: "endDate",
                        visible: false,
                        label: { text: "Ngày hoàn thành" },
                        editorType: "dxDateBox",
                        editorOptions: {
                            pickerType: "rollers",
                            type: "datetime",
                        },
                    },
                    {

                        dataField: "amount",
                        visible: false,
                        label: { text: "Khối lượng" },
                    },
                    {
                        dataField: "unit",
                        visible: false,
                        label: { text: "Đơn vị" },

                    },
                    {
                        dataField: "pmCreateBy",
                        width: "20%",
                        caption: "Người tạo",
                        dataType: "number",
                        allowEditing: false,
                        lookup: {
                            dataSource: customStore_AccountInfo,
                            valueExpr: "accountId",
                            displayExpr: "userName",
                        },
                        editorOptions: {

                            readOnly: true,
                        }
                    },
                    {
                        dataField: "noAttachment",
                        width: "10%",
                        caption: "Người tạo",
                        dataType: "number",
                        allowEditing: false,
                        cellTemplate: (container, options) => {
                            container.append($('<a>').attr("href", "javaScript:void(0)").text(options.displayValue !== null ? options.displayValue + " file(s)" : "chưa có file"));
                        },
                        editorOptions: {

                            readOnly: true,
                        }
                    },
                    {
                        dataField: "priority",
                        caption: "Đánh dấu",
                        //dataType: "bool",
                        //editorType: "dxCheckBox",
                        //showEditorAlways: true,
                        //calculateCellValue: function (rowData) {
                        //    return rowData.priority == 1;
                        //},
                        cellTemplate: function (container, options) {
                            //console.log(container);
                            //console.log(options);
                            $("<div />").dxCheckBox({
                                value: options.value,
                                onValueChanged: function (data) {
                                    var values = { 'priority': (data.value == true) ? 1 : 0 };
                                    customStore(0).store().update(options.row.data.id, values);
                                }
                            }).attr('id', 'CheckBox').appendTo(container);
                        },
                        editorOptions: {
                            //value: true,
                            stylingMode: "filled",
                        },
                    },
                    //{
                    //    type: "buttons",
                    //    //caption: "Tác vụ",
                    //    width: 150,
                    //    alignment: "center",
                    //    //editorOptions: {
                    //    //    value: true,
                    //    //},
                    //    buttons: [{
                    //        template: function(e){
                    //            console.log(e);
                    //            return $("<div>").append($("<b>").append(data.code).addClass("mr-1"), $("<em>").append(data.title));
                    //        }
                    //    }]
                    //}
                ],
                summary: {
                    groupItems: [{
                        column: "pmIsVisible",
                        summaryType: "count"
                    },
                    ]
                },
                editing: {
                    //allowAdding: true,
                    //allowUpdating: true,
                    //allowDeleting: true,
                    mode: "popup",
                    useIcons: true,
                    confirmDelete: true,
                    popup: {
                        width: "50%",
                        height: "50%",
                        showTitle: true,
                        title: "kế hoạch công việc",
                        closeOnOutsideClick: false,
                        showCloseButton: true,
                    },
                    form: {
                        labelLocation: "left",
                        itemType: "group",
                        colCount: 12,
                        showRequiredMark: true,
                        requiredMark: "(*)",
                        items: [
                            {
                                colSpan: 3,
                                dataField: "code",
                                label: { text: "Mã " },
                                editorOptions: {
                                    stylingMode: "filled",
                                    placeholder: "Vui lòng nhập...",
                                },
                            },
                            {
                                colSpan: 5,
                                dataField: "title",
                                label: { text: "Kế hoạch CV" },
                                editorOptions: {
                                    stylingMode: "filled",
                                    placeholder: "Vui lòng nhập...",
                                },
                            },
                            //{
                            //    colSpan: 3,
                            //    dataField: "priority",
                            //    label: { text: "Ưu tiên" },
                            //},
                            {
                                colSpan: 4,
                                dataField: "importantLevel",
                                caption: "Mức độ quan trọng",
                                dataType: "number",
                                lookup: {
                                    dataSource: important,
                                    valueExpr: "ID",
                                    displayExpr: "Name",
                                },
                                editorType: "dxSelectBox",
                                editorOptions: {
                                    stylingMode: "filled",
                                    value: "importantLevel",
                                    dataSource: important,
                                    valueExpr: "ID", displayExpr: "Name",
                                },
                            },
                            {
                                colSpan: 12,
                                dataField: "description",
                                label: { text: "Mô tả CV" },
                                editorType: "dxTextArea",
                                editorOptions: {
                                    stylingMode: "filled",
                                    placeholder: "Vui lòng nhập...",
                                },
                            },
                            //{
                            //    colSpan: 6,
                            //    dataField: "note",
                            //    label: { text: "Lưu ý" },
                            //    editorType: "dxTextArea",
                            //},
                            //{
                            //    colSpan: 6,
                            //    dataField: "startDate",
                            //    label: { text: "Bắt đầu" },
                            //    editorType: "dxDateBox",
                            //    editorOptions: {
                            //        pickerType: "rollers",
                            //        type: "datetime",
                            //    },
                            //},
                            {
                                colSpan: 6,
                                dataField: "endDate",
                                label: { text: "Hoàn thành" },
                                editorType: "dxDateBox",
                                editorOptions: {
                                    stylingMode: "filled",
                                    pickerType: "rollers",
                                    type: "datetime",
                                },
                            },
                            //{
                            //    colSpan: 4,
                            //    dataField: "amount",
                            //    label: { text: "Khối lượng" },
                            //},
                            //{
                            //    colSpan: 4,
                            //    dataField: "unit",
                            //    label: { text: "Đơn vị" },
                            //    lookup: {
                            //        dataSource: unit,
                            //        valueExpr: "ID",
                            //        displayExpr: "Name",
                            //    },
                            //    editorType: "dxSelectBox",
                            //    editorOptions: {
                            //        value: "unit",
                            //        dataSource: unit,
                            //        valueExpr: "ID",
                            //        displayExpr: "Name",
                            //    },
                            //},
                            {
                                colSpan: 4,
                                dataField: "pmIsVisible",
                                label: { text: "Đã Hoàn thành" },
                                editorType: "dxCheckBox",
                                editorOptions: {
                                    stylingMode: "filled",
                                },
                            },
                        ],
                    },
                },
                paging: {
                    enabled: true,
                    pageSize: 20
                },
                pager: {
                    showPageSizeSelector: true,
                    allowedPageSizes: [10, 20, 40],
                    showInfo: true,
                },
                searchPanel: {
                    highlightCaseSensitive: true,
                    highlightSearchText: true,
                    searchVisibleColumnsOnly: true,
                    visible: WIDTH_CONTAINER > 350
                },
                onToolbarPreparing: function (e) {
                    var container = e.component;
                    e.toolbarOptions.items.unshift(
                        {
                            location: "before",
                            widget: "dxSelectBox",
                            options: {
                                dataSource: groupOwner,
                                valueExpr: "ID",
                                displayExpr: "Name",
                                width: "100%",
                                searchEnabled: true,
                                searchMode: "contains",
                                showClearButton: false,
                                stylingMode: "filled",
                                value: GROUPOWNERID,
                                onValueChanged: function (data) {
                                    GROUPOWNERID = data.value;
                                    localStorage.setItem('groupOwnerIdCurrent', GROUPOWNERID);
                                    loadData(GROUPOWNERID);
                                },
                            },
                        },
                        {
                            location: "after",
                            widget: "dxButton",
                            options: {
                                icon: "refresh", tylingMode: "filled", type: "default",
                                onClick: () => container.refresh()
                            },

                        },

                    )
                },
                onInitNewRow: (e) => {
                    e.data.isActive = true;
                    e.data.isVisible = true;
                    //e.data.planProjectId = 3;
                },
                onRowUpdating: (e) => {
                    console.log(e);
                    e.newData['isVisible'] = e.newData['pmIsVisible'];
                    e.newData['id'] = e.oldData['id'];
                },
                onCellPrepared: (e) => {
                    if (e.rowType == "data" && e.columnIndex == 1)
                        e.cellElement.find('.dx-treelist-empty-space').toggleClass("dx-treelist-collapsed", e.data.chilCount > 0)
                },
                onRowDblClick: function (e) {
                    var container = e.component;
                    //CALLPOPUP(
                    //    "Tạo văn bản theo mẫu:",
                    //    "/ThongTin/VanBanMau/_VanBanChiTiet/" + e.data.id,
                    //    ($(window).width() > 767 ? "50%" : "80%"),
                    //    container
                    //);
                },
                selection: {
                    mode: "single"
                },
                onContentReady: (e) => {
                    var container = e.component;
                    $("#action-add").dxSpeedDialAction({
                        index: 1, icon: "fas fa-plus",
                        label: "Thêm mới",
                        visible: PermitInAction["insert"],
                        onClick: () => {
                            CALLPOPUP(
                                "Tạo công việc",
                                "/ThongTin/QLCongViec/_CongViecDetail",
                                ($(window).width() > 767 ? "50%" : "80%"),
                                container
                            );
                        }
                    }).dxSpeedDialAction("instance");

                    DevExpress.config({
                        floatingActionButtonConfig: {
                            icon: "rowfield",
                            label: "Tác vụ",
                            shading: true,
                            position: {
                                of: e.element,
                                my: "right bottom",
                                at: "right bottom",
                                offset: e.component.pageCount() > 1 ? "-15 -60" : "-15 -15"
                            }
                        }
                    });
                    DevExpress.ui.repaintFloatingActionButton();
                },
                onSelectionChanged: (e) => {
                    var selectedRowData = e.selectedRowsData[0];
                    var selectedRowKey = e.selectedRowKeys[0];
                    var container = document.getElementById("div_container");
                    container.classList.replace("col-lg-12", "col-lg-8");
                    container.classList.replace("col-md-12", "col-md-8");
                    var pemistionKey = (selectedRowData.paPermistionId == 9);
                    e.component.refresh();
                    loadData_detail(selectedRowData.id, pemistionKey);
                    //loadAttach(selectedRowData);
                    loadConversation(selectedRowData.id, pemistionKey);
                    loadSchedule(selectedRowData.id);
                    if (selectedRowData != null && selectedRowKey != null) {
                        var data = e.component.getRowIndexByKey(selectedRowData.paId);
                        $("#action-update").dxSpeedDialAction({
                            index: 2, icon: "fas fa-edit", label: "Cập nhật thông tin",
                            visible: PermitInAction["update"] && (selectedRowData.paPermistionId == 9 || selectedRowData.paPermistionId == 7),
                            onClick: () => e.component.editRow(data)
                        }).dxSpeedDialAction("instance");

                        $("#action-delete").dxSpeedDialAction({
                            index: 3,
                            icon: "fas fa-trash",
                            label: "Xoá",
                            visible: (selectedRowData.paId != null && PermitInAction["delete"] && selectedRowData.pmCreateBy == UserCurrentInfo.accountId),
                            onClick: () => {
                                DevExpress.ui.dialog.confirm("Bạn có chắc là muốn hủy bỏ giá trị của hạng mục?", "XÁC NHẬN THÔNG TIN").done(function (dialogResult) {
                                    if (dialogResult) {
                                        var deferred = $.Deferred();
                                        customStore(0).store().remove(selectedRowData.id).then(() => {
                                            DevExpress.ui.notify("Xoá thành công", "success", 3000);
                                            e.component.refresh();
                                        }, deferred.reject);
                                        return deferred.promise();
                                    }
                                });
                            }
                        }).dxSpeedDialAction("instance");
                        $("#action-log").dxSpeedDialAction({
                            index: 4,
                            icon: "fas fa-user",
                            label: "Phân công",
                            visible: (selectedRowData.pmCreateBy === UserCurrentInfo.accountId || selectedRowData.paPermistionId === 7),
                            onClick: () => {
                                console.log(selectedRowData);
                                loadAccountPlanMaster(selectedRowData);
                            }
                        }).dxSpeedDialAction("instance");
                    }
                },
                onCellClick: function (e) {

